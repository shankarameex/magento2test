<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
	<update handle="customer_account" />
	<head>
		<title>My Saved cards</title>
	</head>
	<body>
		<referenceBlock name="page.main.title">
			<action method="setPageTitle">
				<argument translate="true" name="title" xsi:type="string">My Saved
					Credit Cards</argument>
			</action>
		</referenceBlock>
		<referenceContainer name="content">
			<block class="Magento\Framework\View\Element\Template" name="my_email"
				template="Ameex_Authcim::savedcc.phtml" cacheable="false">
			</block>
		</referenceContainer>
	</body>
</page>